@model MuhasebeMaster.MvcWebUI.Models.AccountViewModel
@{
    ViewData["Title"] = "GetAccountDetail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="container">
    <h2>@Model.Account.Fullname</h2>
    <div class="row">
        <div class="col-12 col-sm-12 col-md-6 col-lg-6">
            <ul class="list-group">
                <li class="list-group-item">@Model.Account.Phone</li>
                <li class="list-group-item">@Model.Account.City</li>
                <li class="list-group-item">@Model.Account.District</li>
                <li class="list-group-item">@Model.Account.Address</li>
                <li class="list-group-item">@Model.Account.AddedDate</li>
                <li class="list-group-item">@Model.Account.AccountType</li>
                <li class="list-group-item">

                @ViewBag.Balance @Model.Account.CostType
                </li>
            </ul>
        </div>
    </div>
    @await Component.InvokeAsync("Transaction")
</div>





